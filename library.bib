month = apr,
number = {1},
pages = {15--30},
publisher = {Springer Netherlands},
title = {{A Convergence/Divergence Perspective of Leadership Competencies Managers Believe are Most Important for Success in Organizations: A Cross-Cultural Multilevel Analysis of 40 Countries}},
