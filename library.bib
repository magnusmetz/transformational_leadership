author = {Walker, Matthew and Mondello, Michael J. and Mondell},
file = {:home/magnus/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Walker, Mondello - 2007 - Moving Beyond Economic Impact A Closer Look at the Contingent Valuation Method.pdf:pdf},
