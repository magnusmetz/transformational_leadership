file = {:home/magnus/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ratten, Ratten - 2011 - International sport marketing practical and future research implications.pdf:pdf},
issn = {0885-8624},
journal = {Journal of Business \& Industrial Marketing},
keywords = {entrepreneurship,future research,international,international market trends,sport business ventures,sport marketing,sports},
number = {8},
